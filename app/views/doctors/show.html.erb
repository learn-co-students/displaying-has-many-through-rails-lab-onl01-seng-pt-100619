Doctor Name: <%=@doctor.name%>
Doctor Department: <%=@doctor.department%>

<ul><%@doctor.appointments.each do |appt|%>
  <li><%=appt.appointment_datetime.strftime('%B %d, %Y')%> at <%=appt.appointment_datetime.strftime("%k:%M")%>
  <%= link_to appt.patient.name, patient_path(appt.patient)%></li>
<%end%></ul>